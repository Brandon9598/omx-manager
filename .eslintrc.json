{
  "env": {
    "node": true
  },
  "parserOptions": {
    "ecmaVersion": 5,
    "sourceType": "script"
  },
  "extends": "airbnb-base/legacy",
  "rules": {
    "no-underscore-dangle": "off",
    "max-len": "off",
    "vars-on-top": "off",
    "no-param-reassign": "off",
    "brace-style": [
        "error",
        "stroustrup"
    ]
  }
}
